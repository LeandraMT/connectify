{
    "version": 2,
    "builds": [
    {
      "src": "connectify/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb", "runtime": "python3.11"
      }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "staticfiles"
        }
    }
  ],
    "routes": [
      {
        "src": "/static/(.*)",
      "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "connectify/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "connectify.settings",
      "DJANGO_SECRET_KEY": "django-insecure-0*!evtm*)o440dik0z)(g_#mux1doxa68&tp@(6lze*&tn&bj1"
    }
  }
  
  